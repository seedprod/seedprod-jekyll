---
layout: nil
title : Atom Feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>SeedeProd</title>
 <link href="http://www.seedprod.com/atom.xml" rel="self"/>
 <link href="http://www.seedprod.com"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.seedprod.com</id>
 <author>
   <name>John Turner</name>
   <email>john@seedprod.com</email>
 </author>

 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="{{ site.production_url }}{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.seedprod.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>